{ "name"            : "vault"
, "description"     : "Generates safe passwords for the web"
, "homepage"        : "https://getvau.lt/"
, "author"          : "James Coglan <jcoglan@gmail.com> (http://jcoglan.com/)"
, "keywords"        : ["security", "passwords"]
, "license"         : "GPL-3.0"

, "version"         : "0.4.0"
, "engines"         : {"node": ">=0.10.0"}
, "main"            : "./lib/vault.js"
, "bin"             : {"vault": "./bin/vault"}
, "preferGlobal"    : true

, "dependencies"    : { "mkdirp": "~0.3.0"
                      , "posix-argv-parser": "~1.0.0"
                      , "pw": "~0.0.4"
                      , "rimraf": "~2.2.0"
                      , "ssh-agent": "~0.2.1"
                      , "storeroom": "~0.1.0"
                      }
, "devDependencies" : { "jstest": "" }

, "scripts"         : { "build"       : "wake"
                      , "postinstall" : "./lib/cli/scripts/postinstall"
                      , "test"        : "node spec/node.js"
                      }

, "repository"      : { "type"  : "git"
                      , "url"   : "git://github.com/jcoglan/vault.git"
                      }

, "bugs"            : "http://github.com/jcoglan/vault/issues"
}
