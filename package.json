{ "name"            : "vault"
, "description"     : "Generates safe passwords for the web"
, "homepage"        : "https://vault.jcoglan.com"
, "author"          : "James Coglan <jcoglan@gmail.com> (http://jcoglan.com/)"
, "keywords"        : ["security", "passwords"]

, "version"         : "0.3.0"
, "engines"         : {"node": ">=0.4.0"}
, "main"            : "./lib/vault.js"
, "bin"             : {"vault": "./bin/vault"}
, "preferGlobal"    : true

, "dependencies"    : { "nopt": "~>2.0.0"
                      , "pw": ">=0.0.4"
                      , "remotestorage-oauth": ""
                      , "ssh-agent": ""
                      , "vault-cipher": "~0.3.0"
                      }
, "devDependencies" : { "jsclass": ""
                      }

, "scripts"         : { "postinstall" : "./node/scripts/postinstall"
                      , "test"        : "node spec/node.js"
                      }

, "bugs"            : "http://github.com/jcoglan/vault/issues"

, "licenses"        : [ { "type"  : "MIT"
                        , "url"   : "http://www.opensource.org/licenses/mit-license.php"
                        }
                      ]

, "repositories"    : [ { "type"  : "git"
                        , "url"   : "git://github.com/jcoglan/vault.git"
                        }
                      ]
}

